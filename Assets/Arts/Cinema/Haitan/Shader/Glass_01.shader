				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float3 objToView13 = mul( UNITY_MATRIX_MV, float4( IN.ase_texcoord3.xyz, 1 ) ).xyz;
				float3 normalizeResult14 = normalize( objToView13 );
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float3 break17 = cross( normalizeResult14 , mul( UNITY_MATRIX_V, float4( ase_worldNormal , 0.0 ) ).xyz );
				float2 appendResult18 = (float2(-break17.y , break17.x));
				float2 MatcapUV21 = (appendResult18*0.5 + 0.5);
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float dotResult27 = dot( ase_worldNormal , ase_worldViewDir );
				float2 texCoord50 = IN.ase_texcoord5.xy * float2( 1,1 ) + float2( 0,0 );
				float Thickness43 = saturate( ( ( 1.0 - dotResult27 ) + tex2D( _ThicknessMap, texCoord50 ).r ) );
				float temp_output_33_0 = ( Thickness43 * _RefactIntensity );
				float4 lerpResult38 = lerp( ( _Tint * 0.5 ) , ( _Tint * tex2D( _Matcap, ( MatcapUV21 + temp_output_33_0 ) ) ) , temp_output_33_0);
				float4 tex2DNode5 = tex2D( _Matcap, MatcapUV21 );
				float2 uv_LogoMap = IN.ase_texcoord5.zw * _LogoMap_ST.xy + _LogoMap_ST.zw;
				float4 tex2DNode6 = tex2D( _LogoMap, uv_LogoMap );
				float4 lerpResult56 = lerp( ( lerpResult38 + tex2DNode5 ) , ( tex2D( _LogoMatcap, MatcapUV21 ) * tex2DNode6 * _LogoValue ) , tex2DNode6.a);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = lerpResult56.rgb;
				float Alpha = saturate( ( tex2DNode6.a + max( ( Thickness43 + _AlphaValue ) , tex2DNode5.r ) ) );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
